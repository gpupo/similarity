{
    "name": "gpupo/similarity",
    "description": "Calculate the similarity between strings and numbers working in a different way from a diff tool",
    "keywords": ["similarity", "levenshtein", "compare", "distance", "string", "diff"],
    "license": "MIT",
    "homepage": "https://opensource.gpupo.com/similarity/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": { "Gpupo\\Similarity\\": "src" }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\Tests\\Similarity\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability" : "stable"
}
